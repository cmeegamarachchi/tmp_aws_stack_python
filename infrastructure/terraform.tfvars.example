# Example Terraform variables file
# Copy this file to terraform.tfvars and customize for your deployment

# AWS region where resources will be created
aws_region = "us-east-1"

# Environment name (dev, staging, prod)
environment = "dev"

# Project name (used in resource naming)
project_name = "contacts-app"

# Optional: Custom domain name for the application
# domain_name = "contacts.yourdomain.com"
